            <div class="row">
                <asp:ContentPlaceHolder ID="cpDashboard" runat="server">
                </asp:ContentPlaceHolder>
            </div>
            <div class="row">
                <asp:ContentPlaceHolder ID="cpSearch" runat="server">
                </asp:ContentPlaceHolder>
            </div>
        </div>
        <!-- /#page-wrapper -->
