                                            <ItemTemplate>
                                                <li>
                                                    <a href="#"> <%# Container.DataItem.ToString().Split('|')[1] %><span class="fa arrow"></span></a>                                    
                                                    <ul class="nav nav-third-level" data="<%# Container.DataItem %>" runat="server" id="ulReports">
                                                        <asp:Repeater ID="rReportLinks" runat="server" >
                                                            <ItemTemplate>
                                                                <li>
                                                                    <%--<asp:LinkButton ID="btnFetchReport" runat="server" Text="<%# Path.GetFileName(Container.DataItem.ToString()).Split('_').ToList().LastOrDefault() %>"/>--%>
                                                                    <a href="<%# Container.DataItem %>" target="_blank"> <%# GenericHelper.GetReportDisplayName(Container.DataItem.ToString()) %></a> 
                                                                </li>                                                                                           
                                                            </ItemTemplate>
                                                        </asp:Repeater>
                                                    
                                                    </ul>
                                                </li>
                                            </ItemTemplate>
                                        </asp:Repeater>
                                    </ul>
                                 </li>
                            </ItemTemplate>
                        </asp:Repeater>
                        
                       
                        <li>
                            <a href="#"><i class="fa fa-files-o fa-fw"></i> Archived Reports<span class="fa arrow"></span></a>  
