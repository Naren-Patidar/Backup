<DTS:Property DTS:Name="CreationName">Microsoft.SqlServer.Dts.Tasks.ExecuteSQLTask.ExecuteSQLTask, Microsoft.SqlServer.SQLTask, Version=10.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91</DTS:Property>
<DTS:Property DTS:Name="DisableEventHandlers">0</DTS:Property><DTS:ObjectData><SQLTask:SqlTaskData SQLTask:Connection="{6887AD2C-D174-4438-9914-B0E0819218CE}" SQLTask:TimeOut="0" SQLTask:IsStoredProc="False" SQLTask:BypassPrepare="False" SQLTask:SqlStmtSourceType="DirectInput" SQLTask:SqlStatementSource="UPDATE OfflineStagingData SET CashierID = '0000'&#xA;&#xA;INSERT INTO TempOfflineDataDuplicate(TransactionDateTime,ClubcardID,TescoStoreID,SourcePOSID,CashierID,SourceSystemTransactionID,AmountSpent,TotalPoints,TempTxnID,BonusPoints)&#xA;select TransactionDateTime,ClubcardID,TescoStoreID,SourcePOSID,CashierID,SourceSystemTransactionID,AmountSpent,TotalPoints,TempTxnID,BonusPoints&#xA;from OfflineStagingData &#xA;where ClubcardID in (select ClubcardID from OfflineStagingData WHERE ClubcardID != 0 AND ClubcardID IS NOT NULL Group by ClubcardID,TransactionDateTime,TescoStoreID,SourcePOSID,&#xA;SourceSystemTransactionID having count(1)&gt;1)&#xA;&#xA;&#xA;&#xA;delete from OfflineStagingData where OfflineStagingData.ClubcardID in &#xA;(select ClubcardID from OfflineStagingData WHERE ClubcardID != 0 AND ClubcardID IS NOT NULL Group by ClubcardID,TransactionDateTime,TescoStoreID,SourcePOSID,SourceSystemTransactionID having count(1)&gt;1)&#xA;&#xA;--To check card number is within the existing card range or not&#xA;&#xA;&#xA;insert into dbo.TempOfflineDataOutOfRange(TransactionDateTime,ClubcardID,TescoStoreID,SourcePOSID,CashierID,SourceSystemTransactionID,AmountSpent,TotalPoints,TempTxnID,BonusPoints)&#xA;select TransactionDateTime,ClubcardID,TescoStoreID,SourcePOSID,CashierID,SourceSystemTransactionID,AmountSpent,TotalPoints,TempTxnID,BonusPoints&#xA;from OfflineStagingData &#xA;where ClubcardID not in (select ClubcardID from OfflineStagingData T,ClubcardRange &#xA;where Convert(bigint,T.ClubcardID) between Convert(bigint,MinCardNumber) and convert(bigint,MaxCardNumber) &#xA;And ClubcardRange.IsDeleted = 'N' AND (ClubcardID != 0 AND ClubcardID IS NOT NULL)) AND (ClubcardID != 0 AND ClubcardID IS NOT NULL)&#xA;&#xA;		&#xA;delete from OfflineStagingData where OfflineStagingData.TempTxnID in (select TempTxnID from TempOfflineDataOutOfRange)&#xA;&#xA;&#xA;insert into dbo.TempOfflineDateAmtNeg(TransactionDateTime,ClubcardID,TescoStoreID,SourcePOSID,CashierID,SourceSystemTransactionID,AmountSpent,TotalPoints,TempTxnID,BonusPoints)&#xA;select TransactionDateTime,ClubcardID,TescoStoreID,SourcePOSID,CashierID,SourceSystemTransactionID,AmountSpent,TotalPoints,TempTxnID,BonusPoints&#xA;from OfflineStagingData &#xA;where AmountSpent&lt;0 and TotalPoints&gt;0&#xA;&#xA;&#xA;delete from OfflineStagingData where AmountSpent&lt;0 and TotalPoints&gt;0&#xA;&#xA;--amoount spent is pasitive points is negetive&#xA;&#xA;&#xA;insert into dbo.TempOfflineDatePointNeg(TransactionDateTime,ClubcardID,TescoStoreID,SourcePOSID,CashierID,SourceSystemTransactionID,AmountSpent,TotalPoints,TempTxnID,BonusPoints)&#xA;select TransactionDateTime,ClubcardID,TescoStoreID,SourcePOSID,CashierID,SourceSystemTransactionID,AmountSpent,TotalPoints,TempTxnID,BonusPoints&#xA;from OfflineStagingData &#xA;where AmountSpent&gt;0 and TotalPoints&lt;0&#xA;&#xA;&#xA;delete from OfflineStagingData where AmountSpent&gt;0 and TotalPoints&lt;0&#xA;&#xA;--amount spent is zero&#xA;&#xA;&#xA;insert into dbo.TempOfflineDateAmtzero(TransactionDateTime,ClubcardID,TescoStoreID,SourcePOSID,CashierID,SourceSystemTransactionID,AmountSpent,TotalPoints,TempTxnID,BonusPoints)&#xA;select TransactionDateTime,ClubcardID,TescoStoreID,SourcePOSID,CashierID,SourceSystemTransactionID,AmountSpent,TotalPoints,TempTxnID,BonusPoints&#xA;from OfflineStagingData &#xA;where AmountSpent=0 and TotalPoints&gt;0&#xA;&#xA;&#xA;delete from OfflineStagingData where AmountSpent=0 and TotalPoints&gt;0&#xA;&#xA;--card_account number does not exists&#xA;&#xA;&#xA;&#xA;insert into dbo.TempOfflineDateFieldNull(TransactionDateTime,ClubcardID,TescoStoreID,SourcePOSID,CashierID,SourceSystemTransactionID,AmountSpent,TotalPoints,TempTxnID,BonusPoints)&#xA;select TransactionDateTime,ClubcardID,TescoStoreID,SourcePOSID,CashierID,SourceSystemTransactionID,AmountSpent,TotalPoints,TempTxnID,BonusPoints from OfflineStagingData &#xA;where ClubcardID is null or TransactionDateTime is null or TescoStoreID is null or SourcePOSID is null or CashierID is null &#xA;or SourceSystemTransactionID is null or AmountSpent is null or TotalPoints is null&#xA;&#xA;&#xA;delete from OfflineStagingData where OfflineStagingData.TempTxnID in (select TempTxnID from TempOfflineDateFieldNUll)&#xA;&#xA;/* Set Rejection Code to 4 if date is future date */&#xA;&#xA;insert into dbo.TempOfflineDateFuture(TransactionDateTime,ClubcardID,TescoStoreID,SourcePOSID,CashierID,SourceSystemTransactionID,AmountSpent,TotalPoints,TempTxnID,BonusPoints) &#xA;select TransactionDateTime,ClubcardID,TescoStoreID,SourcePOSID,CashierID,SourceSystemTransactionID,AmountSpent,TotalPoints,TempTxnID,BonusPoints from OfflineStagingData &#xA;where TransactionDateTime&gt;getdate()&#xA;&#xA;delete from OfflineStagingData where OfflineStagingData.TempTxnID in (select TempTxnID from TempOfflineDateFuture)&#xA;&#xA;/* Set Rejection Code to 15 if store is unknown */&#xA;&#xA;insert into dbo.TempOfflineStoreUnknown(TransactionDateTime,ClubcardID,TescoStoreID,SourcePOSID,CashierID,SourceSystemTransactionID,AmountSpent,TotalPoints,TempTxnID,BonusPoints) &#xA;select TransactionDateTime,ClubcardID,TescoStoreID,SourcePOSID,CashierID,SourceSystemTransactionID,AmountSpent,TotalPoints,TempTxnID,BonusPoints from OfflineStagingData &#xA;where OfflineStagingData.TescoStoreID not in (select TescoStoreID from TescoStore (nolock))&#xA;&#xA;&#xA;delete from OfflineStagingData where OfflineStagingData.TempTxnID in (select TempTxnID from TempOfflineStoreUnknown)&#xA;&#xA;&#xA;/* Transaction already present */&#xA;&#xA;insert into TempOfflineDupTxn(TransactionDateTime,ClubcardID,TescoStoreID,SourcePOSID,CashierID,SourceSystemTransactionID,AmountSpent,TotalPoints,TempTxnID,BonusPoints)&#xA;select T.TransactionDateTime,T.ClubcardID,T.TescoStoreID,T.SourcePOSID,T.CashierID,T.SourceSystemTransactionID,T.AmountSpent,T.TotalPoints,T.TempTxnID,T.BonusPoints &#xA;from OfflineStagingData T , ClubcardTransaction tx&#xA;	Where&#xA;	convert(datetime,convert(varchar(10),T.TransactionDateTime,111)) = convert(datetime,convert(varchar(10),tx.TransactionDateTime,111))&#xA;	And Cast(T.SourcePOSID As Int)=  Cast(tx.SourcePOSID As Int) &#xA;	And Cast(T.SourceSystemTransactionID As Int)  =  Cast(tx.SourceSystemTransactionID As Int) &#xA;	AND T.TescoStoreID = tx.TescoStoreID&#xA;&#xA;&#xA;&#xA;&#xA;delete from OfflineStagingData where OfflineStagingData.TempTxnID in (select TempTxnID from TempOfflineDupTxn)&#xA;&#xA;/* Invalid SourcePOSID or CashierID */&#xA;&#xA;insert into TempOfflineDataInvalidPos(TransactionDateTime,ClubcardID,TescoStoreID,SourcePOSID,CashierID,SourceSystemTransactionID,AmountSpent,TotalPoints,TempTxnID,BonusPoints)&#xA;select TransactionDateTime,ClubcardID,TescoStoreID,SourcePOSID,CashierID,SourceSystemTransactionID,AmountSpent,TotalPoints,TempTxnID,BonusPoints &#xA;from OfflineStagingData where len(OfflineStagingData.SourcePOSID)&gt;4 or len(OfflineStagingData.CashierID)&gt;=5&#xA;&#xA;delete from OfflineStagingData where OfflineStagingData.TempTxnID in (select TempTxnID from TempOfflineDataInvalidPos)&#xA;&#xA;&#xA;/* For AlternateID invalid */&#xA;&#xA;insert into dbo.TempOfflineInvalidAlternateID(TransactionDateTime,ClubcardID,TescoStoreID,SourcePOSID,CashierID,SourceSystemTransactionID,AmountSpent,TotalPoints,TempTxnID,BonusPoints,AlternateID) &#xA;select TransactionDateTime,ClubcardID,TescoStoreID,SourcePOSID,CashierID,SourceSystemTransactionID,AmountSpent,TotalPoints,TempTxnID,BonusPoints,AlternateID from OfflineStagingData &#xA;where (Offlinestagingdata.clubcardID = ' ' or Offlinestagingdata.clubcardID is null or Offlinestagingdata.clubcardID = 0) and (OfflineStagingData.AlternateID is not null or OfflineStagingData.AlternateID != ' ' ) and (OfflineStagingdata.AlternateID NOT IN(select MobilePhoneNumber from Customer where MobilePhoneNumber is Not Null or MobilePhoneNumber != 0))&#xA;&#xA;&#xA;delete from OfflineStagingData where OfflineStagingData.TempTxnID in (select TempTxnID from TempOfflineInvalidAlternateID)&#xA;&#xA;-- For invalid AlternateID&#xA;&#xA;" SQLTask:CodePage="1252" SQLTask:ResultType="ResultSetType_None" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask"/></DTS:ObjectData></DTS:Executable>
