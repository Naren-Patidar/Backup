<DTS:Property DTS:Name="DisableEventHandlers">0</DTS:Property><DTS:ObjectData><SQLTask:SqlTaskData SQLTask:Connection="{6887AD2C-D174-4438-9914-B0E0819218CE}" SQLTask:TimeOut="0" SQLTask:IsStoredProc="False" SQLTask:BypassPrepare="True" SQLTask:SqlStmtSourceType="DirectInput" SQLTask:SqlStatementSource="IF((SELECT Count(1) from TempOfflineSkeleton) &gt; 0)&#xA;BEGIN&#xA;	Declare @cnt BIGINT  &#xA;	Declare @row Int  &#xA;	declare @custId bigint  &#xA;	declare @customerId bigint &#xA;	declare @clubcardId bigint &#xA;	declare @error char(1)&#xA;	declare @UserID BIGINT  &#xA;	DECLARE @@THISERROR INT &#xA;	DECLARE @DefaultDataProtectionPreference INT&#xA;	SET @DefaultDataProtectionPreference = 0&#xA;	select  @UserID = UserID from ApplicationUser where UserName='ngcadmin' and IsDeleted = 'N'  &#xA;	SET @error = '0'&#xA;	  &#xA;&#xA;	set @row = 1  &#xA;	Create TABLE #CreateCustomer(ClubcardID varchar(50) COLLATE Database_Default,  &#xA;	CustomerID BIGINT,CardType Int,TransactionDateTime DateTime)  &#xA;	Insert into #CreateCustomer&#xA;	select ClubcardID, '','',TransactionDateTime from TempOfflineSkeleton &#xA;&#xA;	  &#xA;	update #CreateCustomer  &#xA;	set CardType = cp.ClubcardType from clubcardrange cr inner join ClubcardType cp on cr.ClubcardType=cp.clubcardtype  &#xA;	where ClubcardID &gt;= cr.mincardnumber and ClubcardID &lt;=cr.maxCardNumber and   &#xA;	cp.CardNumberLength = len(ClubcardID)  &#xA;	  &#xA;	update TempOfflineSkeleton  &#xA;	set ErrorReason = 'Card Number out of range' from #CreateCustomer  inner join TempOfflineSkeleton on #CreateCustomer.ClubcardID=  &#xA;	TempOfflineSkeleton.ClubcardID  where #CreateCustomer.CardType = 0  &#xA;&#xA;	delete from #CreateCustomer where cardtype = 0  &#xA;&#xA;	Create TABLE #TempCustomerCards(Rowid int identity(1,1),ClubcardID varchar(50) COLLATE Database_Default,  &#xA;	CustomerID BIGINT,CardType Int,TransactionDateTime DateTime)  &#xA;&#xA;	Insert into #TempCustomerCards&#xA;	Select * from #CreateCustomer&#xA;	  &#xA;	Select @cnt = count(*) from #TempCustomerCards  &#xA;	  &#xA;	while (@row &lt;=@cnt)  &#xA;	Begin  &#xA;&#xA;		SET @error = '0'&#xA;		SELECT @clubcardId = ClubcardID from   #TempCustomerCards where rowid=@row    &#xA;		IF(SELECT COUNT(1) FROM Clubcard WHERE Clubcardid = @clubcardId) = 0&#xA;		BEGIN&#xA;		  INSERT INTO customer (TitleEnglish,Name1,Sex,RaceId,MailingAddressLine5,BusinessAddressLine5,CustomerMailStatus,BusinessType  &#xA;		  ,PreferredStoreID,JoinedDate,JoinedStoreID,CustomerUseStatusID,InsertDateTime,InsertBy,AmendDateTime,AmendBy,IsDeleted  &#xA;		  ,CustomerWelcomedFlag)  &#xA;		  select 'Unknown','Name Unknown','U',0,'-1','-1',1,0,'-1',TransactionDateTime,'-1',0,getdate(),@UserID,getdate(),@UserID,'N','0' from #TempCustomerCards where rowid=@row  &#xA;&#xA;		  SET @@THISERROR = @@ERROR  &#xA;		  IF (@@THISERROR != 0) &#xA;		  BEGIN &#xA;				Update TempOfflineSkeleton set ErrorReason = 'INSERT Customer failed' Where ClubcardID = @clubcardId&#xA;				SET @error = '1'&#xA;		  END &#xA;&#xA;		  set @custId = scope_identity() &#xA;		&#xA;		  UPDATE Customer set PrimaryCustomerID = @custId where CustomerID = @custId &#xA;		  SET @@THISERROR = @@ERROR  &#xA;		IF (@@THISERROR != 0) &#xA;		BEGIN &#xA;			Update TempOfflineSkeleton set ErrorReason = 'UPDATE Customer failed' Where ClubcardID = @clubcardId&#xA;			SET @error = '1'&#xA;		END &#xA;		IF(@error = '0')&#xA;		BEGIN&#xA;		INSERT INTO Clubcard (CustomerID,ClubcardID,ClubcardStatus,InsertDateTime,CardIssuedDate,InsertBy,ClubcardType,AmendDateTime,AmendBy,IsDeleted,PrimaryClubcardID )   &#xA;			select @custId, ClubcardID, 0, getdate(),TransactionDateTime,@UserID,CardType,getdate(),@UserID,'N',ClubcardID from   #TempCustomerCards where rowid=@row    &#xA;		SET @@THISERROR = @@ERROR  &#xA;		IF (@@THISERROR != 0) &#xA;		BEGIN &#xA;			Update TempOfflineSkeleton set ErrorReason = 'INSERT Clubcard failed' Where ClubcardID = @clubcardId&#xA;			SET @error = '1'	&#xA;		END&#xA;		END&#xA;&#xA;	 &#xA;		SELECT @customerId = CustomerID FROM Clubcard WHERE Clubcardid = @clubcardId&#xA;		IF(@error = '0')&#xA;		BEGIN&#xA;			INSERT INTO CustomerAlternateID (CustomerID, CustomerAlternateIDType, CustomerAlternateID,InsertDateTime, InsertBy, AmendDateTime, AmendBy, IsDeleted)    &#xA;			VALUES (@customerId,1,NULL,getdate(),@UserID,getdate(),@UserID,'N' )    &#xA;&#xA;			DECLARE @RowCounts INT  &#xA;			SET @RowCounts = 0  &#xA;			Select @RowCounts = COUNT(*) from CustomerPreference WHERE CustomerID = @customerId   &#xA;			AND PreferenceID IN (SELECT PreferenceID FROM Preference WHERE PreferenceType = 3)  &#xA;			IF (@RowCounts &lt; 1 )  &#xA;			BEGIN  &#xA;			INSERT INTO CustomerPreference(   &#xA;			  CustomerID,   &#xA;			  PreferenceID,   &#xA;			  InsertDateTime,  &#xA;			  InsertBy,        &#xA;			  AmendDateTime,  &#xA;			  AmendBy,  &#xA;			  IsDeleted)   &#xA;			VALUES (@customerId, 13 , getdate(), @UserID, getdate(), @UserID,'N')   &#xA;		END  &#xA;	  &#xA;		IF( @DefaultDataProtectionPreference = '1')  &#xA;		BEGIN  &#xA;			INSERT INTO CustomerPreference (CustomerID, PreferenceID, InsertDateTime, InsertBy, AmendDateTime, AmendBy, IsDeleted )   &#xA;			VALUES (@customerId,'27',getdate(),@UserID,getdate(),@UserID,'N')  &#xA;&#xA;			INSERT INTO CustomerPreference (CustomerID, PreferenceID, InsertDateTime, InsertBy, AmendDateTime, AmendBy, IsDeleted )   &#xA;			VALUES (@customerId,'28',getdate(),@UserID,getdate(),@UserID,'N')  &#xA;&#xA;			INSERT INTO CustomerPreference (CustomerID, PreferenceID, InsertDateTime, InsertBy, AmendDateTime, AmendBy, IsDeleted )   &#xA;			VALUES (@customerId,'29',getdate(),@UserID,getdate(),@UserID,'N')  &#xA;&#xA;			INSERT INTO CustomerPreference (CustomerID, PreferenceID, InsertDateTime, InsertBy, AmendDateTime, AmendBy, IsDeleted )   &#xA;			VALUES (@customerId,'30',getdate(),@UserID,getdate(),@UserID,'N')  &#xA;&#xA;			INSERT INTO CustomerPreference (CustomerID, PreferenceID, InsertDateTime, InsertBy, AmendDateTime, AmendBy, IsDeleted )   &#xA;			VALUES (@customerId,'31',getdate(),@UserID,getdate(),@UserID,'N')  &#xA;&#xA;			INSERT INTO CustomerPreference (CustomerID, PreferenceID, InsertDateTime, InsertBy, AmendDateTime, AmendBy, IsDeleted )   &#xA;			VALUES (@customerId,'32',getdate(),@UserID,getdate(),@UserID,'N')  &#xA;&#xA;			INSERT INTO CustomerPreference (CustomerID, PreferenceID, InsertDateTime, InsertBy, AmendDateTime, AmendBy, IsDeleted )   &#xA;			VALUES (@customerId,'33',getdate(),@UserID,getdate(),@UserID,'N')  &#xA;&#xA;			INSERT INTO CustomerPreference (CustomerID, PreferenceID, InsertDateTime, InsertBy, AmendDateTime, AmendBy, IsDeleted )   &#xA;			VALUES (@customerId,'34',getdate(),@UserID,getdate(),@UserID,'N')  &#xA;&#xA;			INSERT INTO CustomerPreference (CustomerID, PreferenceID, InsertDateTime, InsertBy, AmendDateTime, AmendBy, IsDeleted )   &#xA;			VALUES (@customerId,'35',getdate(),@UserID,getdate(),@UserID,'N')  &#xA;&#xA;			INSERT INTO CustomerPreference (CustomerID, PreferenceID, InsertDateTime, InsertBy, AmendDateTime, AmendBy, IsDeleted )   &#xA;			VALUES (@customerId,'36',getdate(),@UserID,getdate(),@UserID,'N')  &#xA;&#xA;			INSERT INTO CustomerPreference (CustomerID, PreferenceID, InsertDateTime, InsertBy, AmendDateTime, AmendBy, IsDeleted )   &#xA;			VALUES (@customerId,'37',getdate(),@UserID,getdate(),@UserID,'N')  &#xA;&#xA;			INSERT INTO CustomerPreference (CustomerID, PreferenceID, InsertDateTime, InsertBy, AmendDateTime, AmendBy, IsDeleted )   &#xA;			VALUES (@customerId,'38',getdate(),@UserID,getdate(),@UserID,'N')  &#xA;		END  &#xA;		END&#xA;		END    &#xA;		SET @customerId = null&#xA;		SET @clubcardId = null&#xA;		set @row = @row + 1  &#xA;		end  &#xA;	  &#xA;		Update TempOfflineSkeleton  &#xA;		Set ErrorReason = NULL where ErrorReason = 'Invalid CardNumber'  &#xA;&#xA;		DROP TABLE #TempCustomerCards&#xA;		DROP TABLE #CreateCustomer&#xA;END" SQLTask:CodePage="1252" SQLTask:ResultType="ResultSetType_None" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask"/></DTS: