        public void Main()
        {
            String sBatchEndMessage = Dts.Variables["BatchEndMessage"].Value.ToString();
            String sPackageNameMessage = Dts.Variables["PackageNameMessage"].Value.ToString();
            String sPackageName = Dts.Variables["PackageName"].Value.ToString();
            String sBatchStartTime = Dts.Variables["BatchStartTime"].Value.ToString();
            String sBatchStartMessage = Dts.Variables["BatchStartMessage"].Value.ToString();
            String sSourceMessage = Dts.Variables["SourceMessage"].Value.ToString();
            String sNoOfDateFuture = Dts.Variables["NoOfDateFuture"].Value.ToString();
            String sNoOfStoreUnknown = Dts.Variables["NoOfStoreUnknown"].Value.ToString();
            String sNoOfAlternateIDInvalid = Dts.Variables["NoOfAlternateIDInvalid"].Value.ToString();
            String sNoofNoCurrentOffer = Dts.Variables["NoofNoCurrentOffer"].Value.ToString();
            String sNoOfSkeleton = Dts.Variables["NoOfSkeleton"].Value.ToString();
            String sNoOfDupTxn = Dts.Variables["NoOfDupTxn"].Value.ToString();
            String sNoOfNegAmt = Dts.Variables["NoOfNegAmt"].Value.ToString();
            String sNoOfNegPtn = Dts.Variables["NoOfNegPtn"].Value.ToString();
            String sNoOfZeroAmt = Dts.Variables["NoOfZeroAmt"].Value.ToString();
            String sNoOfOutOfRange = Dts.Variables["NoOfOutOfRange"].Value.ToString();
            String sNoOfStoreNoWrong = Dts.Variables["NoOfStoreNoWrong"].Value.ToString();
            String sNoOfInvalidPos = Dts.Variables["NoOfInvalidPos"].Value.ToString();
            String sNoOfDuplicate = Dts.Variables["NoOfDuplicate"].Value.ToString();
            String sNoofValidTxns = Dts.Variables["NoofValidTxns"].Value.ToString();

            EventLog eventLog;
            eventLog = new EventLog();
            eventLog.Source = sSourceMessage;
            try
            {

                String msg = "";
                String sBatchEndTime = String.Format("{0:F}", DateTime.Now);
                eventLog.WriteEntry(sPackageNameMessage + ": " + sPackageName + "\n" + sBatchStartMessage + ": " + sBatchStartTime + "\n" + msg, EventLogEntryType.Information);
                eventLog.WriteEntry("No of Transaction with Future Date:" + sNoOfDateFuture + "\n" + "No of Transaction with Unknown Store:" + sNoOfStoreUnknown + "\n" + "No of Skeleton Cards:" + sNoOfSkeleton + "\n" + "No of Duplicate Transaction:" + sNoOfDupTxn + "\n" + "No of Transaction with Negative Amount:" + sNoOfNegAmt + "\n" + "No of Transaction with Negative Points:" + sNoOfNegPtn + "\n" + "No of Transaction with Zero Amount:" + sNoOfZeroAmt + "\n" + "No of Invalid Cards:" + sNoOfOutOfRange + "\n" + "No of Invalid Store Number:" + sNoOfStoreNoWrong + "\n" + "No of invalid pos number:" + sNoOfInvalidPos + "\n" + "No of Transactions Inserted:" + sNoofValidTxns, EventLogEntryType.Information);
                eventLog.WriteEntry(sBatchEndMessage + ": " + sBatchEndTime, EventLogEntryType.Information);
                string sLogFileName = CreateFilePath(Dts.Variables["LogFilePath"].Value.ToString() + "OfflineTransactionsBatch");

                if (File.Exists(sLogFileName))
                    File.Delete(sLogFileName);
                string sMessage = "";
                sMessage = "No of Transaction with Future Date:" + sNoOfDateFuture;
                CreateLogFile(sMessage, sLogFileName);
                sMessage = "No of Transaction with Unknown Store:" + sNoOfStoreUnknown;
                CreateLogFile(sMessage, sLogFileName);
                sMessage = "No of Transaction with Invalid AlternateID:" + sNoOfAlternateIDInvalid;
                CreateLogFile(sMessage, sLogFileName);
                sMessage = "No of Transaction with No Current Offer:" + sNoofNoCurrentOffer;
                CreateLogFile(sMessage, sLogFileName);
                sMessage = "No of Skeleton Cards:" + sNoOfSkeleton;
                CreateLogFile(sMessage, sLogFileName);
                sMessage = "No of Duplicate Transaction:" + sNoOfDupTxn;
                CreateLogFile(sMessage, sLogFileName);
                sMessage = "No of Transaction with Negative Amount:" + sNoOfNegAmt;
                CreateLogFile(sMessage, sLogFileName);
                sMessage = "No of Transaction with Negative Points:" + sNoOfNegPtn;
                CreateLogFile(sMessage, sLogFileName);
                sMessage = "No of Transaction with Zero Amount:" + sNoOfZeroAmt;
                CreateLogFile(sMessage, sLogFileName);
                sMessage = "No of Invalid Cards:" + sNoOfOutOfRange;
                CreateLogFile(sMessage, sLogFileName);
                sMessage = "No of invalid pos number:" + sNoOfInvalidPos;
                CreateLogFile(sMessage, sLogFileName);
                sMessage = "No of Transactions Inserted:" + sNoofValidTxns;
                CreateLogFile(sMessage, sLogFileName);

                Dts.TaskResult = (int)ScriptResults.Success;
            }
            catch (Exception ex)
            {
                eventLog.WriteEntry("Error Description :" + ex.Message.ToString(), EventLogEntryType.Error);
                Dts.TaskResult = (int)ScriptResults.Failure;
            }
        }

        public void CreateLogFile(string sLogMessage, string sLogFileName)
        {


            StreamWriter sFile = new StreamWriter(sLogFileName, true);
            sFile.WriteLine(sLogMessage);
            sFile.Close();
        }


        private string CreateFilePath(string sFileName)
        {
            string sHour = "";
            string sMinute = "";
            string sSecond = "";
            string oFileName = "";

            if (DateTime.Now.Hour.ToString().Length < 2)
            {
                sHour = "0" + DateTime.Now.Hour.ToString();
            }
            else
            {
                sHour = DateTime.Now.Hour.ToString();
            }

            if (DateTime.Now.Minute.ToString().Length < 2)
            {
                sHour = "0" + DateTime.Now.Minute.ToString();
            }
            else
            {
                sHour = DateTime.Now.Minute.ToString();
            }
            if (DateTime.Now.Second.ToString().Length < 2)
            {
                sHour = "0" + DateTime.Now.Second.ToString();
            }
            else
            {
                sHour = DateTime.Now.Second.ToString();
            }
            if (DateTime.Now.Month.ToString().Length < 2)
            {
                sHour = "0" + DateTime.Now.Month.ToString();
            }
            else
            {
                sHour = DateTime.Now.Month.ToString();
            }
            if (DateTime.Now.Day.ToString().Length < 2)
            {
                sHour = "0" + DateTime.Now.Day.ToString();
            }
            else
            {
                sHour = DateTime.Now.Day.ToString();
            }
            oFileName = sFileName + "_" + DateTime.Now.Year.ToString() + DateTime.Now.Month.ToString() + DateTime.Now.Day.ToString() + sHour.ToString() + sMinute.ToString() + sSecond.ToString() + ".txt";
            return oFileName;
        }
