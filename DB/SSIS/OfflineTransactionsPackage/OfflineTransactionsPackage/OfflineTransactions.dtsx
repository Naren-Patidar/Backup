<DTS:Property DTS:Name="CreationName">Microsoft.SqlServer.Dts.Tasks.ExecuteSQLTask.ExecuteSQLTask, Microsoft.SqlServer.SQLTask, Version=10.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91</DTS:Property>
<DTS:Property DTS:Name="DisableEventHandlers">0</DTS:Property><DTS:ObjectData><SQLTask:SqlTaskData SQLTask:Connection="{6887AD2C-D174-4438-9914-B0E0819218CE}" SQLTask:TimeOut="0" SQLTask:IsStoredProc="False" SQLTask:BypassPrepare="True" SQLTask:SqlStmtSourceType="DirectInput" SQLTask:SqlStatementSource="DECLARE @NoOfInserted INT&#xA;Declare @cmd varchar(2000)&#xA;Declare @OfferID SMALLINT&#xA;Select @OfferID=OfferID from Offer where ((Offer.StartDateTime &lt;= GETDATE()) AND(Offer.EndDateTime &gt; GETDATE()-1)) And IsDeleted ='N'&#xA;Declare @type int &#xA;Declare @amount decimal(15,2),@points int&#xA;&#xA;       --Modified by Sridevi - Wrt New DB Design&#xA;		 DECLARE @TransactionalDBName NVARCHAR(100);&#xA;		 SET @TransactionalDBName =  dbo.TransactionalDBName();  &#xA;&#xA;&#xA;-- To Delete all Transactions from OfflineStagingData if there is no Current Offer&#xA;&#xA;if (@offerID is null)&#xA;insert into dbo.TempOfflineNoCurrentOffer(TransactionDateTime,ClubcardID,TescoStoreID,SourcePOSID,CashierID,SourceSystemTransactionID,AmountSpent,TotalPoints,TempTxnID,BonusPoints) &#xA;select TransactionDateTime,ClubcardID,TescoStoreID,SourcePOSID,CashierID,SourceSystemTransactionID,AmountSpent,TotalPoints,TempTxnID,BonusPoints from OfflineStagingData &#xA;&#xA;delete from OfflineStagingData where OfflineStagingData.TempTxnID in (select TempTxnID from TempOfflineNoCurrentOffer)&#xA;--&#xA;&#xA;Truncate table TempOfflineData_AddTxns&#xA;Truncate Table tempPCData&#xA;&#xA;Insert into  TempOfflineData_AddTxns (&#xA;		TransactionDateTime,ClubcardID, TescoStoreID, &#xA;		SourcePOSID, CashierID, SourceSystemTransactionID, &#xA;		AmountSpent, TotalPoints,BonusPoints, CustomerID, &#xA;		StoreWelcomePoints,TransactionType, &#xA;		TransactionSourceID, UserID, TransactionReasonID,&#xA;		PrimaryCustomerID,ProductPoints,ExtraPoints1,&#xA;		ExtraPoints2,CustomerWelcomedFlag,CardNumber&#xA;		)&#xA;select &#xA;	TransactionDateTime, PrimaryClubcardID, OfflineStagingData.TescoStoreID,&#xA;	REPLICATE('0', 4 - len(OfflineStagingData.SourcePOSID)) + OfflineStagingData.SourcePOSID, CashierID, &#xA;	REPLICATE('0', 10 - len(OfflineStagingData.SourceSystemTransactionID)) + SourceSystemTransactionID, &#xA;	ISNull(AmountSpent,0), ISNull(TotalPoints, 0),ISNULL(BonusPoints,0),customer.CustomerID ,0,&#xA;	8,6, NULL, 1,customer.PrimaryCustomerID, 0, 0, 0,CustomerWelcomedFlag,OfflineStagingData.ClubcardID&#xA;From OfflineStagingData &#xA;        Inner join Clubcard on OfflineStagingData.ClubcardID = Clubcard.ClubcardID&#xA;        Inner join customer&#xA;        on Clubcard.CustomerID = customer.CustomerID&#xA;        Inner join TescoStore&#xA;        on TescoStore.TescoStoreID = OfflineStagingData.TescoStoreID &#xA;&#xA;&#xA;Update TempOfflineData_AddTxns Set StoreWelcomePoints = ISNull(StoreWelcomePointsQty,0)&#xA;From TescoStore inner join TempOfflineData_AddTxns on TescoStore.TescoStoreID = TempOfflineData_AddTxns.TescoStoreID&#xA;where TransactionDateTime in (select min(TransactionDateTime) from TempOfflineData_AddTxns&#xA;group by ClubcardID)&#xA;&#xA;&#xA;Update TempOfflineData_AddTxns &#xA;Set TransactionType = 3 &#xA;Where AmountSpent&lt;0 and TotalPoints&lt;0&#xA;&#xA;Update TempOfflineData_AddTxns &#xA;Set StoreWelcomePoints = 0&#xA;Where CustomerID in (select Customer.CustomerID from Customer left outer join TempOfflineData_AddTxns on Customer.CustomerID=TempOfflineData_AddTxns.CustomerID where Customer.CustomerWelcomedFlag='1')&#xA;&#xA;Declare @userid int &#xA;Select @userid=UserID from ApplicationUser Where Upper(UserName)='SYSTEM'&#xA;&#xA;Set @cmd = 'Insert into tempPCData &#xA;			select distinct PrimaryCustomerID,ClubcardID&#xA;			from TempOfflineData_AddTxns A &#xA;			Where  A.PrimaryCustomerID not in (select CustomerID from Clubcard Where ClubcardID in &#xA;					(select ClubcardID from ' + @TransactionalDBName +'.dbo.ClubcardOffer_'+Convert(varchar(10),@OfferID)+'))'&#xA;Exec(@cmd)&#xA;&#xA;Select @cmd = 'INSERT INTO ' + @TransactionalDBName +'.dbo.ClubcardOffer_'+Convert(varchar(10),@OfferID)+' ( &#xA;			'+ @TransactionalDBName +'.dbo.ClubcardOffer_'+Convert(varchar(10),@OfferID)+'.SKUPointsQty,'+ @TransactionalDBName +'.dbo.ClubcardOffer_'+Convert(varchar(10),@OfferID)+'.HighRewardInd, '+ @TransactionalDBName +'.dbo.ClubcardOffer_'+Convert(varchar(10),@OfferID)+'.RewardInd,  &#xA;			'+ @TransactionalDBName +'.dbo.ClubcardOffer_'+Convert(varchar(10),@OfferID)+'.WelcomePointsQty, '+ @TransactionalDBName +'.dbo.ClubcardOffer_'+Convert(varchar(10),@OfferID)+'.BonusPointsQty, '+ @TransactionalDBName +'.dbo.ClubcardOffer_'+Convert(varchar(10),@OfferID)+'.RewardReissuedDate, &#xA;			'+ @TransactionalDBName +'.dbo.ClubcardOffer_'+Convert(varchar(10),@OfferID)+'.RewardreissuedInd, '+ @TransactionalDBName +'.dbo.ClubcardOffer_'+Convert(varchar(10),@OfferID)+'.RewardReissueRequestedDate, &#xA;			'+ @TransactionalDBName +'.dbo.ClubcardOffer_'+Convert(varchar(10),@OfferID)+'.SignOffInd, '+ @TransactionalDBName +'.dbo.ClubcardOffer_'+Convert(varchar(10),@OfferID)+'.PointsCarriedForward, '+ @TransactionalDBName +'.dbo.ClubcardOffer_'+Convert(varchar(10),@OfferID)+'.PointsBroughtForward, &#xA;			'+ @TransactionalDBName +'.dbo.ClubcardOffer_'+Convert(varchar(10),@OfferID)+'.PartnerPointsBalanceQty, '+ @TransactionalDBName +'.dbo.ClubcardOffer_'+Convert(varchar(10),@OfferID)+'.ClubcardID,'+ @TransactionalDBName +'.dbo.ClubcardOffer_'+Convert(varchar(10),@OfferID)+'.RewardReissueRequestedBy, &#xA;			'+ @TransactionalDBName +'.dbo.ClubcardOffer_'+Convert(varchar(10),@OfferID)+'.VoucherRedeemedValue,'+ @TransactionalDBName +'.dbo.ClubcardOffer_'+Convert(varchar(10),@OfferID)+'.AmendDatetime,'+ @TransactionalDBName +'.dbo.ClubcardOffer_'+Convert(varchar(10),@OfferID)+'.AmendBy,&#xA;			'+ @TransactionalDBName +'.dbo.ClubcardOffer_'+Convert(varchar(10),@OfferID)+'.InsertDateTime,'+ @TransactionalDBName +'.dbo.ClubcardOffer_'+Convert(varchar(10),@OfferID)+'.InsertBy,'+ @TransactionalDBName +'.dbo.ClubcardOffer_'+Convert(varchar(10),@OfferID)+'.IsDeleted,&#xA;			'+ @TransactionalDBName +'.dbo.ClubcardOffer_'+Convert(varchar(10),@OfferID)+'.MailedFlag,'+ @TransactionalDBName +'.dbo.ClubcardOffer_'+Convert(varchar(10),@OfferID)+'.ConvertedFlag)&#xA;			select distinct 0, ''0'', ''0'', 0, 0, NULL,''0'', NULL, ''0'', 0, 0, 0, PrimaryClubcardID, 0, 0,&#xA;					getdate(),'+Convert(varchar(10),@userid)+',getdate(),'+Convert(varchar(10),@userid)+',''N'',''0'',''0''&#xA;			from tempPCData T left outer join Clubcard on CustomerID=PrimaryCustomerID&#xA;			Where PrimaryCustomerID in (select PrimaryCustomerID from tempPCData) '&#xA;Exec(@cmd) &#xA;&#xA;truncate table tempPCData&#xA;&#xA;Insert into tempPCData select distinct (A.PrimaryCustomerID),ClubcardID&#xA;From TempOfflineData_AddTxns A &#xA;	Inner Join customer C&#xA;		On A.CustomerID = C.CustomerID&#xA;Where C.CustomerWelcomedFlag is null or A.CustomerWelcomedFlag = '0' and &#xA;	  A.TransactionDateTime = (select min(TransactionDateTime) from TempOfflineData_AddTxns where A.PrimaryCustomerID = PrimaryCustomerID)     &#xA;&#xA;truncate table tempDate&#xA;&#xA;Insert into tempDate&#xA;select ClubcardID,min(TransactionDateTime) from TempOfflineData_AddTxns group by ClubcardID &#xA;&#xA;truncate table TempOfflineCardNumber&#xA;&#xA;Insert into TempOfflineCardNumber(primaryCustomerID,ClubcardID)&#xA;Select Distinct CustomerID,PrimaryClubcardID From Clubcard where CustomerID in (&#xA;select PrimaryCustomerID From customer where CustomerID in (select CustomerID from tempofflinedata_addtxns))&#xA;&#xA;Insert into TempOfflineTotalPoints&#xA;select TempOfflineData_AddTxns.PrimaryCustomerID,sum(TotalPoints),SUM(TempOfflineData_AddTxns.BonusPoints) from TempOfflineData_AddTxns,TempOfflineCardNumber &#xA;Where TempOfflineData_AddTxns.PrimaryCustomerID = TempOfflineCardNumber.primaryCustomerID &#xA; group by TempOfflineData_AddTxns.PrimaryCustomerID&#xA;&#xA;Update TempOfflineCardNumber Set PointsBalance = TempOfflineTotalPoints.TotalPoints&#xA;From TempOfflineData_AddTxns ,TempOfflineTotalPoints&#xA;Where TempOfflineData_AddTxns.PrimaryCustomerID = TempOfflineCardNumber.primaryCustomerID &#xA;	And TempOfflineCardNumber.primaryCustomerID = TempOfflineTotalPoints.primaryCustomerID&#xA;&#xA;Update TempOfflineCardNumber Set TempOfflineCardNumber.BonusPoints = TempOfflineTotalPoints.BonusPoints &#xA;From TempOfflineData_AddTxns ,TempOfflineTotalPoints&#xA;Where TempOfflineData_AddTxns.PrimaryCustomerID = TempOfflineCardNumber.PrimaryCustomerID &#xA;	And TempOfflineCardNumber.PrimaryCustomerID = TempOfflineTotalPoints.PrimaryCustomerID&#xA;&#xA;Update TempOfflineCardNumber Set WelcomePoints = StoreWelcomePoints &#xA;From TempOfflineData_AddTxns ,TempOfflineTotalPoints&#xA;Where TempOfflineData_AddTxns.PrimaryCustomerID = TempOfflineCardNumber.PrimaryCustomerID &#xA;	And TempOfflineCardNumber.PrimaryCustomerID = TempOfflineTotalPoints.PrimaryCustomerID&#xA;	And StoreWelcomePoints != 0&#xA;&#xA;Set @cmd = 'UPDATE '+ @TransactionalDBName +'.dbo.ClubcardOffer_'+Convert(varchar(10),@OfferID)+'&#xA;			SET '+ @TransactionalDBName +'.dbo.ClubcardOffer_'+Convert(varchar(10),@OfferID)+'.WelcomePointsQty = ISNull(WelcomePointsQty,0) + (select Isnull(WelcomePoints ,0)&#xA;			from TempOfflineCardNumber Where TempOfflineCardNumber.ClubcardID = C.ClubcardID and IsDeleted=''N'')&#xA;			From '+ @TransactionalDBName +'.dbo.ClubcardOffer_'+Convert(varchar(10),@OfferID)+' C, TempOfflineCardNumber T1&#xA;			Where T1.ClubcardID = C.ClubcardID and IsDeleted=''N'''&#xA;Exec(@cmd)&#xA;&#xA;Select @cmd = 'update '+ @TransactionalDBName +'.dbo.ClubcardOffer_'+Convert(varchar(10),@OfferID)+'&#xA;			set PointsBalanceQty = ISNull(PointsBalanceQty,0) + (select Sum(IsNull(PointsBalance,0) + ISNULL(BonusPoints,0)) &#xA;			from TempOfflineCardNumber Where TempOfflineCardNumber.ClubcardID = C.ClubcardID and IsDeleted=''N'')&#xA;			From '+ @TransactionalDBName +'.dbo.ClubcardOffer_'+Convert(varchar(10),@OfferID)+' C, TempOfflineCardNumber T1&#xA;			Where T1.ClubcardID = C.ClubcardID and IsDeleted=''N'''&#xA;Exec(@cmd)&#xA;&#xA;Set @cmd = 'update '+ @TransactionalDBName +'.dbo.ClubcardOffer_'+Convert(varchar(10),@OfferID)+'&#xA;			set PointsBalanceQty = ISNull(PointsBalanceQty,0) + (select Isnull(WelcomePoints ,0) &#xA;			from TempOfflineCardNumber Where TempOfflineCardNumber.ClubcardID = C.ClubcardID and IsDeleted=''N'')&#xA;			From '+ @TransactionalDBName +'.dbo.ClubcardOffer_'+Convert(varchar(10),@OfferID)+' C, TempOfflineCardNumber T1&#xA;			Where T1.ClubcardID = C.ClubcardID and IsDeleted=''N'''&#xA;Exec(@cmd)&#xA;&#xA;Set @cmd = 'update '+ @TransactionalDBName +'.dbo.ClubcardOffer_'+Convert(varchar(10),@OfferID)+'&#xA;			set BonusPointsQty = ISNull(BonusPointsQty,0) + (select Isnull(BonusPoints ,0) &#xA;			from TempOfflineCardNumber Where TempOfflineCardNumber.ClubcardID = C.ClubcardID and IsDeleted=''N'')&#xA;			From '+ @TransactionalDBName +'.dbo.ClubcardOffer_'+Convert(varchar(10),@OfferID)+' C, TempOfflineCardNumber T1&#xA;			Where T1.ClubcardID = C.ClubcardID and IsDeleted=''N'''&#xA;Exec(@cmd)&#xA;&#xA;update	customer &#xA;set		customer.CustomerWelcomedFlag = '1' &#xA;from 	TempOfflineData_AddTxns &#xA;		inner Join&#xA;		Customer on TempOfflineData_AddTxns.CustomerID = Customer.CustomerID&#xA;where 	TempOfflineData_AddTxns.CustomerWelcomedFlag = '0' or TempOfflineData_AddTxns.CustomerWelcomedFlag is null&#xA;		and Customer.IsDeleted = 'N'&#xA; &#xA;&#xA;Select @userid=UserID from ApplicationUser where UPPER(UserName) = 'System'&#xA;Select @cmd = 'Insert into '+ @TransactionalDBName +'.dbo.ClubcardTransaction_'+Convert(varchar(10),@OfferID)+' (&#xA;			ClubcardID, TescoStoreID, TransactionDateTime, TransactionType ,&#xA;			SourcePOSID ,SourceSystemTransactionID,CashierID,InsertBy,&#xA;			AmountSpent,TransactionReasonID,WelcomePointsQty,SKUPointsQty,&#xA;			ManualPointsQty,GreenPointsQty,InsertDateTime, &#xA;			PartnerOutletID,AmendDateTime,AmendBy,IsDeleted,NormalPoints,BonusPointsQty)&#xA;		 select   &#xA;			A.CardNumber,A.TescoStoreID,&#xA;			A.TransactionDateTime,A.TransactionType,A.SourcePOSID,&#xA;			A.SourceSystemTransactionID,A.CashierID,'+Convert(varchar(10),@userid)+',A.AmountSpent/100,A.TransactionReasonID,&#xA;			A.StoreWelcomePoints,A.ProductPoints, A.ExtraPoints1 ,A.ExtraPoints2,&#xA;			getdate(),NULL,getdate(),'+Convert(varchar(10),@userid)+',''N'',TotalPoints,BonusPoints&#xA;		from TempOfflineData_AddTxns A'&#xA;Exec(@cmd)&#xA;Select @NoOfInserted=count(1) from TempOfflineData_AddTxns&#xA;&#xA;UPDATE StoreDay &#xA;SET StoreDay.AmendDateTime=getdate(),StoreDay.AmendBy=@userid,&#xA;	StoreDay.WelcomePointsBalance = ISNull(StoreDay.WelcomePointsBalance,0) + &#xA;	(select IsNull(sum(StoreWelcomePoints),0) from TempOfflineData_AddTxns Where TempOfflineData_AddTxns.TescoStoreID =StoreDay.TescoStoreID And &#xA;	CONVERT(DATETIME,CONVERT(VARCHAR, TempOfflineData_AddTxns.TransactionDateTime ,111),111) = StoreDay.StoreDate And  TempOfflineData_AddTxns.RejectionCode= 32),  &#xA;	StoreDay.AmountSpentBalance = ISNull(StoreDay.AmountSpentBalance,0) + (select Sum(IsNull(AmountSpent,0)) from TempOfflineData_AddTxns Where TempOfflineData_AddTxns.TescoStoreID =StoreDay.TescoStoreID And &#xA;	CONVERT(DATETIME,CONVERT(VARCHAR, TempOfflineData_AddTxns.TransactionDateTime ,111),111) = StoreDay.StoreDate), &#xA;	StoreDay.NormalPointsBalance = ISNull(StoreDay.NormalPointsBalance,0) + (select Sum(IsNull(TotalPoints,0)) from TempOfflineData_AddTxns Where TempOfflineData_AddTxns.TescoStoreID =StoreDay.TescoStoreID And &#xA;	CONVERT(DATETIME,CONVERT(VARCHAR, TempOfflineData_AddTxns.TransactionDateTime ,111),111) = StoreDay.StoreDate),&#xA;	StoreDay.NumberOfTxns = ISNull(StoreDay.NumberOfTxns,0) + (select Count(TempOfflineData_AddTxns.TransactionDateTime) from TempOfflineData_AddTxns Where TempOfflineData_AddTxns.TescoStoreID =StoreDay.TescoStoreID And &#xA;	CONVERT(DATETIME,CONVERT(VARCHAR, TempOfflineData_AddTxns.TransactionDateTime ,111),111) = StoreDay.StoreDate), &#xA;	StoreDay.SKUPointsQty = ISnull(SKUPointsQty,0) ,&#xA;	StoreDay.BonusPointsQty = ISnull(BonusPointsQty,0) ,&#xA;	StoreDay.PartnerPointsBalanceQty = ISnull(PartnerPointsBalanceQty,0) ,&#xA;	StoreDay.ManualTotalPointsBalance = ISnull(ManualTotalPointsBalance,0)&#xA;	From StoreDay, TempOfflineData_AddTxns T1&#xA;	Where T1.TescoStoreID =StoreDay.TescoStoreID And CONVERT(DATETIME,CONVERT(VARCHAR, T1.TransactionDateTime ,111),111) = StoreDay.StoreDate And StoreDay.IsDeleted = 'N'&#xA;&#xA;INSERT INTO StoreDay ( StoreDay.StoreDate, StoreDay.SKUPointsQty, StoreDay.NumberOfnewhscCustomers, &#xA;	StoreDay.NumberOfNormalCustomers, StoreDay.NormalPointsBalance, &#xA;	StoreDay.NumberOfCustomers, StoreDay.ValueOfRedeemedVouchers, &#xA;	StoreDay.WelcomePointsBalance, StoreDay.BonusPointsQty, StoreDay.NumberOfMailPromotionCustomers, &#xA;	StoreDay.ManualTotalPointsBalance, StoreDay.AmountSpentBalance,&#xA;	StoreDay.NumberOfThirdPartyMailPromotionCustomers, StoreDay.NumberOfDeceasedCustomers, StoreDay.NumberOfRedeemedVouchers, &#xA;	StoreDay.NumberOfManualTxns, StoreDay.NumberOfBannedCustomers, StoreDay.NumberOfGroupPromotionCustomers, &#xA;	StoreDay.PartnerPointsBalanceQty, StoreDay.NumberOfPrimaryCustomers, StoreDay.NumberOfPhonePromotionCustomers, &#xA;	StoreDay.NumberOfTxns, StoreDay.TescoStoreID, StoreDay.NumberOfPrimaryNormalCustomers, StoreDay.NumberOfNewCustomers, &#xA;	StoreDay.NumberOfLeftCustomers, StoreDay.NumberOfInactiveCustomers,StoreDay.InsertDateTime,StoreDay.InsertBy,&#xA;	StoreDay.AmendDateTime,StoreDay..AmendBy ,StoreDay.IsDeleted) &#xA;select CONVERT(DATETIME,CONVERT(VARCHAR, TransactionDateTime ,111),111), 0, 0, 0, sum(TotalPoints), 0, 0, &#xA;(select iSnULL(sum(StoreWelcomePoints),0) from TempOfflineData_AddTxns &#xA;where TempOfflineData_AddTxns.RejectionCode = 32 and TempOfflineData_AddTxns.TescoStoreID =A.TescoStoreID&#xA;And CONVERT(DATETIME,CONVERT(VARCHAR, TempOfflineData_AddTxns.TransactionDateTime ,111),111) = CONVERT(DATETIME,CONVERT(VARCHAR, A.TransactionDateTime ,111),111)) , &#xA;	0, 0, 0, sum(AmountSpent), 0, 0, 0, 0, 0, 0, 0, 0, 0, count(*), A.TescoStoreID, 0, 0, 0, 0 ,getdate(),@userid,getdate(),@userid,'N'&#xA;From TempOfflineData_AddTxns A&#xA;	Left Join StoreDay&#xA;		on CONVERT(DATETIME,CONVERT(VARCHAR, A.TransactionDateTime ,111),111) = StoreDay.StoreDate	&#xA;		And A.TescoStoreID = StoreDay.TescoStoreID	&#xA;	Where (StoreDay.TescoStoreID Is Null Or StoreDay.StoreDate Is Null)&#xA;	group by CONVERT(DATETIME,CONVERT(VARCHAR, A.TransactionDateTime ,111),111), A.TescoStoreID &#xA;&#xA;&#xA;" SQLTask:CodePage="1252" SQLTask:ResultType="ResultSetType_None" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask"/></DTS:ObjectData></DTS:Executable>
