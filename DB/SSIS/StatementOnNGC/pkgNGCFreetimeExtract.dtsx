<property id="5" name="SqlCommand" dataType="System.String" state="default" isArray="false" description="The SQL command to be executed." typeConverter="" UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=10.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91" containsID="false" expressionType="None">select 
 A.ClubcardID,
 A.HouseholdID,
 A.CustomerTitle,
 Substring(A.CustomerName1,1,1)as MainCustomerInitials,
 A.CustomerName3,
 '' As AssociateCustomerTitle,
 '' As AssociateCustomerInitials,
 '' As AssociateCustomerSurname ,
 A.AddressLine1,
 A.AddressLine2,
 A.AddressLine3,
 A.City,
 A.Province,
 A.PostCode,
 'S'  As CurrentDealStatus,
CASE WHEN ISNULL(B.TotalPoints,0) &gt;= 150 THEN 
0.01* (isnull(B.TotalPoints,0) - (isnull(B.TescoCarriedForwardPoints,0) + isnull(B.TescoBankCarriedForwardPoints,0))) 
ELSE 0 END
as RewardValue,
'0' As AverageWeeklyQualifyingSpend,
'0' As UnusedRewardValue,
'0'As UsedRewardValue,
'V' AirmilesFlag,
 (ISNULL(B.TescoBankRewardMiles,0)+ISNULL(B.TescoRewardMiles,0))as RewardAirMiles,
 Convert(VARCHAR(40),MembershipNumber) As MembershipNumber
 
 from MailableCustomers A with (Nolock)
 inner join PointDetails B ON A.HouseholdID = B.HouseholdID
 LEFT OUTER join TempClubMembership T ON T.HouseholdID=A.HouseholdID
 where VirginAtlantic='1'</property>
