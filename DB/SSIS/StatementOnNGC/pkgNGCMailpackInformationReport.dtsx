<properties>
<property id="450" name="CommandTimeout" dataType="System.Int32" state="default" isArray="false" description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out." typeConverter="" UITypeEditor="" containsID="false" expressionType="None">0</property>
<property id="451" name="OpenRowset" dataType="System.String" state="default" isArray="false" description="Specifies the name of the database object used to open a rowset." typeConverter="" UITypeEditor="" containsID="false" expressionType="None"></property>
<property id="452" name="OpenRowsetVariable" dataType="System.String" state="default" isArray="false" description="Specifies the variable that contains the name of the database object used to open a rowset." typeConverter="" UITypeEditor="" containsID="false" expressionType="None"></property>
<property id="453" name="SqlCommand" dataType="System.String" state="default" isArray="false" description="The SQL command to be executed." typeConverter="" UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=10.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91" containsID="false" expressionType="None">SELECT MailPack,RewardType,NumberIssued,VoucherVale1And4,VoucherVale2And5,VoucherVale3And6,
Having1Vouchers,Having2Vouchers,CASE WHEN RewardTYpe LIKE '%High%' THEN NumberIssued ELSE Having3Vouchers END Having3Vouchers, 
SUM(VoucherVale1And4+VoucherVale2And5+VoucherVale3And6)
 TotalReward FROM 
(Select Mailpack, M.RewardType,  COUNT(DISTINCT M.HouseholdID) NumberIssued, SUM(ISNULL(E1.VoucherValue,0)) as VoucherVale1And4,
SUM(E2.VoucherValue) as VoucherVale2And5,SUM(ISNULL(E3.VoucherValue,0)) as VoucherVale3And6,
SUM(CASE WHEN M.RewardTYpe LIKE '%High%' THEN 0 
WHEN VoucherReference1 IS NOT NULL AND VoucherReference2 IS NULL  AND VoucherReference3 IS NULL THEN 1 ELSE 0 END) Having1Vouchers,
SUM(CASE WHEN M.RewardTYpe LIKE '%High%' THEN 0 WHEN VoucherReference1 IS NOT NULL AND VoucherReference2 IS NOT NULL  AND VoucherReference3 IS NULL THEN 1 ELSE 0 END) Having2Vouchers,
SUM(CASE WHEN VoucherReference1 IS NOT NULL AND VoucherReference2 IS NOT NULL AND VoucherReference3 IS NOT NULL THEN 1 ELSE 0 END) Having3Vouchers

FROM MailableCustomers (nolock) M 
INNER JOIN TempVoucherForMailingFile (nolock) ON M.HouseholdID  = HHCCID
LEFT OUTER JOIN  EANVoucherType E1 ON  ( VoucherReference1 = E1.VoucherEAN )
 LEFT OUTER JOIN   EANVoucherType E2 ON  (  VoucherReference2 = E2.VoucherEAN)
 LEFT OUTER JOIN  EANVoucherType E3 ON   ( VoucherReference3 = E3.VoucherEAN)
 
 INNER JOIN PointDetails (nolock) PD ON PD.HouseholdID = M.HouseholdID  GROUP BY Mailpack, M.RewardType) SV
 Group by  MailPack,RewardType,NumberIssued,VoucherVale1And4,VoucherVale2And5,VoucherVale3And6,
Having1Vouchers,Having2Vouchers,Having3Vouchers</property>
